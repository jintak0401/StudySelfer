{
  "expo": {
    "name": "SWmaestro",
    "slug": "SWmaestro",
    "version": "1.0.0",
    "backgroundColor": "#4F62C0",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4F62C0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg"]
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.StudySelfer.YourTutor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#4F62C0"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
